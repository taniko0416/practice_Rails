<h1>People#Edit</h1>
<%# <p>Find me in app/views/people/index.html.erb</p> %>

<p><%= @msg %></p>



<table>

<%= form_for(@person,url:{controller:'people',action:'update',id:@person.id}) do |form| %>
    <tr>
        <th>Name</th>
        <td><%= form.text_filed :name %></td>
        <th></th>
    </tr>
    <% @data.each do |obj| %>
    <tr>
        <td><%= obj.id %></td>
        <td><a href="/people/<%= obj.id %>"><%= obj.name %></a></td>
        <td><a href="/people/edit/<%= obj.id %>">Edit</a></td>
    </tr>
    <% end %>
</table>
