<h1>People#index</h1>
<%# <p>Find me in app/views/people/index.html.erb</p> %>

<p><%= @msg %></p> 
<pre>
    <table>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
        <% @data.each do |obj| %>
        <tr>
            <td><%= obj.id %></td>
            <td><a href="/people/<%= obj.id %>"><%= obj.name %></a></td>
        </tr>
        <% end %>
    </table>
</pre>